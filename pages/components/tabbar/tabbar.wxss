.com-tabbar {
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
  height: 50px;
  overflow: hidden;
  display: flex;
  border-top: 1px solid #eee;
}

.com-tabbar-item {
  flex: 1;
  text-align: center;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  color: #333;
}

.com-tabbar-icon {
  width: 28px;
  height: 28px;
}

.com-tabbar-icon-super{
  width: 36px;
  height: 36px;
}

.com-tabbar-text{
  margin-top: 4px;
  font-size: 10px;
  line-height: 10px;
}

.com-tabbar-item_active{
  color: #fdb975;
}